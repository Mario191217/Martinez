        <td>
            @Html.DisplayFor(modelItem => item.Proyecto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rentabilidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Terminado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdProyecto }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdProyecto }) |
